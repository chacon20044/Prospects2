{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "default",
  "rsc": true,
  "tsx": false,
  "tailwind": {
    "config": "tailwind.config.js",
    "css": "app/globals.css",
    "baseColor": "neutral",
    "cssVariables": true,
    "prefix": ""
  },
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils",
    "ui": "@/components/ui",
    "lib": "@/lib",
    "hooks": "@/hooks"
  },
    "table": {
      "path": "@/components/ui/table",
      "exports": [
        "Table",
        "TableBody",
        "TableCell",
        "TableHead",
        "TableHeader",
        "TableRow"
      ]
    },
    "iconLibrary": "lucide" 
}
